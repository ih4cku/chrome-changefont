{
  "manifest_version": 2,

  "name": "Change Font",
  "description": "Change web page font.",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "content_scripts":[{
      "matches": ["<all_urls>"],
      "js": ["jquery.min.js", "cs.js"]
  }],
  "permissions": [
    "activeTab",
    "<all_urls>"
  ]
}

